<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" 
    android:background="#40000000">
    

    <RelativeLayout
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:layout_centerInParent="true"
        android:orientation="vertical" >

        <RelativeLayout
            android:id="@+id/rl"
            android:layout_width="200dp"
            android:layout_height="38dp"
            android:background="#000000"
            android:gravity="center_vertical|center_horizontal" >

            <ImageView
                android:id="@+id/image"
                android:layout_width="28dp"
                android:layout_height="28dp"
                android:layout_centerVertical="true"
                android:background="@drawable/ic_question_position" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="15dp"
                android:layout_toRightOf="@id/image"
                android:text="@string/question_select_position"
                android:textColor="#ffffff"
                android:textSize="16sp" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/rl1"
            android:layout_width="200dp"
            android:layout_height="38dp"
            android:layout_below="@id/rl"
            android:background="#ffffff" >

            <TextView
                android:id="@+id/text1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/question_engine"
               android:textColor="#5B5B5B"
                android:layout_centerInParent="true"
                android:textSize="16sp" />
            <TextView
                android:layout_width="fill_parent"
                android:layout_height="1dp"
                android:layout_marginTop="4dp"
                android:layout_below="@id/text1"
                android:background="#E0E0E0" />
        </RelativeLayout>
         <RelativeLayout
             android:id="@+id/rl2"
            android:layout_width="200dp"
            android:layout_height="38dp"
            android:layout_below="@id/rl1"
            android:background="#ffffff" >

            <TextView
                android:id="@+id/text1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/question_crankcase"
               android:textColor="#5B5B5B"
                android:layout_centerInParent="true"
                android:textSize="16sp" />
            <TextView
                android:layout_width="fill_parent"
                android:layout_height="1dp"
                android:layout_marginTop="4dp"
                android:layout_below="@id/text1"
                android:background="#E0E0E0" />
        </RelativeLayout>
         <RelativeLayout
             android:id="@+id/rl3"
            android:layout_width="200dp"
            android:layout_height="38dp"
            android:layout_below="@id/rl2"
            android:background="#ffffff" >

            <TextView
                android:id="@+id/text1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/question_cylinder_heat"
               android:textColor="#5B5B5B"
                android:layout_centerInParent="true"
                android:textSize="16sp" />
            <TextView
                android:layout_width="fill_parent"
                android:layout_height="1dp"
                android:layout_marginTop="4dp"
                android:layout_below="@id/text1"
                android:background="#E0E0E0" />
        </RelativeLayout>
         <RelativeLayout
             android:id="@+id/rl4"
            android:layout_width="200dp"
            android:layout_height="38dp"
            android:layout_below="@id/rl3"
            android:background="#ffffff" >

            <TextView
                android:id="@+id/text1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/question_cylinder_liner"
               android:textColor="#5B5B5B"
                android:layout_centerInParent="true"
                android:textSize="16sp" />
            <TextView
                android:layout_width="fill_parent"
                android:layout_height="1dp"
                android:layout_marginTop="4dp"
                android:layout_below="@id/text1"
                android:background="#E0E0E0" />
        </RelativeLayout>
         <RelativeLayout
             android:id="@+id/rl5"
            android:layout_width="200dp"
            android:layout_height="38dp"
            android:layout_below="@id/rl4"
            android:background="#ffffff" >

            <TextView
                android:id="@+id/text1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/question_ohv"
               android:textColor="#5B5B5B"
                android:layout_centerInParent="true"
                android:textSize="16sp" />
            <TextView
                android:layout_width="fill_parent"
                android:layout_height="1dp"
                android:layout_marginTop="4dp"
                android:layout_below="@id/text1"
                android:background="#E0E0E0" />
        </RelativeLayout>
         <RelativeLayout
             android:id="@+id/rl6"
            android:layout_width="200dp"
            android:layout_height="38dp"
            android:layout_below="@id/rl5"
            android:background="#ffffff" >

            <TextView
                android:id="@+id/text1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/question_knock_sensor"
               android:textColor="#5B5B5B"
                android:layout_centerInParent="true"
                android:textSize="16sp" />
            <TextView
                android:layout_width="fill_parent"
                android:layout_height="1dp"
                android:layout_marginTop="4dp"
                android:layout_below="@id/text1"
                android:background="#E0E0E0" />
        </RelativeLayout>
        <RelativeLayout
             android:id="@+id/rl7"
            android:layout_width="200dp"
            android:layout_height="38dp"
            android:layout_below="@id/rl6"
            android:background="#ffffff" >

            <TextView
                android:id="@+id/text1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/question_sparking_plug"
               android:textColor="#5B5B5B"
                android:layout_centerInParent="true"
                android:textSize="16sp" />
            <TextView
                android:layout_width="fill_parent"
                android:layout_height="1dp"
                android:layout_marginTop="4dp"
                android:layout_below="@id/text1"
                android:background="#E0E0E0" />
        </RelativeLayout>
         <RelativeLayout
             android:id="@+id/rl8"
            android:layout_width="200dp"
            android:layout_height="38dp"
            android:layout_below="@id/rl7"
            android:background="#ffffff" >

            <TextView
                android:id="@+id/text1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/question_camshaft"
                android:textColor="#5B5B5B"
                android:layout_centerInParent="true"
                android:textSize="16sp" />
            <TextView
                android:layout_width="fill_parent"
                android:layout_height="1dp"
                android:layout_marginTop="4dp"
                android:layout_below="@id/text1"
                android:background="#E0E0E0" />
        </RelativeLayout>
         <RelativeLayout
             android:id="@+id/rl9"
            android:layout_width="200dp"
            android:layout_height="38dp"
            android:layout_below="@id/rl8"
            android:background="#ffffff" >

            <TextView
                android:id="@+id/text1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/question_distributor"
               android:textColor="#5B5B5B"
                android:layout_centerInParent="true"
                android:textSize="16sp" />
            <TextView
                android:layout_width="fill_parent"
                android:layout_height="1dp"
                android:layout_marginTop="4dp"
                android:layout_below="@id/text1"
                android:background="#E0E0E0" />
        </RelativeLayout>
        <RelativeLayout
             android:id="@+id/rl10"
            android:layout_width="200dp"
            android:layout_height="38dp"
            android:layout_below="@id/rl9"
            android:layout_marginTop="-5dp"
            android:background="#E0E0E0" >
            <TextView
                android:id="@+id/text1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/cancle"
                android:textColor="#ffffff"
                android:layout_centerInParent="true"
                android:textSize="16sp" />
        </RelativeLayout>
    </RelativeLayout>

</RelativeLayout>